<% unless @inactive_locations.empty? %>
  <div class="panel clearfix">
    <h5>Inactive Locations</h5>
    <ul class="side-nav">
      <% @inactive_locations.each do |location| %>
        <li><%= link_to location.location_name, location_path(location) %></li>
      <% end %>
    </ul>
  </div>
<% end %>